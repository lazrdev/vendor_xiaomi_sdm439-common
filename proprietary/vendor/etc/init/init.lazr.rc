# Tweaks by lazrdev

on property:sys.boot_completed=1

# Set Low Memory Killer to Light preset at boot

write /sys/module/lowmemorykiller/parameters/minfree 9374,14061,18748,23435,28122,32809

# Enable High Performance DAC by akirasupr

write /sys/module/wcd9335_dlkm/parameters/cpe_debug_mode 1

# Enable Low Distort AMP

write /sys/module/wcd9335_dlkm/parameters/low_distort_amp 1

# Input Boost

write /sys/module/cpu_boost/parameters/input_boost_ms 500

# Image Enhancement

write /sys/devices/virtual/graphics/fb0/msm_fb_dispparam 0010
